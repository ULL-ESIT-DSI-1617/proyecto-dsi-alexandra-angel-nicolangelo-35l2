<html>
 <head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="../css/global.css">
    <script src="../js/main.js"></script>
  </head>
<body>
    <h1 id="headerLogin">Login Temperatura</h1>
    <br>
    <div id="wrapForm">
        <form method="POST" action="/login">
        <label for="username">Usuario:</label>
        <input type="text" name="username" id="username" />
        <p id="emptyUsername" class="errorInfo">* Campo obligatorio</p>
        <br><br>

        <label for="password">Contraseña:</label>
        <input type="password" id="password" name="password" />
        <p id="emptyPassword" class="errorInfo">* Campo obligatorio</p>
        <br><br>

        <button type="submit" id="loginButton"> Iniciar sesión </button>
    </div>
  </body>
</html>
